{
    "manifest_version": 2,
    "name": "Tab sleep timer",
    "version": "0.1",
    "description": "Programa uma aba do navegador para ser fechada depois de um per√≠odo de tempo;",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "alarms"],
    "background": {
        "scripts": [
            "background.js"
        ],
        "permissions": [
            "activeTab",
            "alarms"
        ]
    }
}