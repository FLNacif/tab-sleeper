{
    "manifest_version": 2,
    "name": "Tab sleeper",
    "version": "0.2.15",
    "description": "Schedule a chrome tab to close after a period of time in minutes.",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "alarms",
        "storage"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}